# Organize usings
dotnet_sort_system_directives_first = true # IDE0055
dotnet_separate_import_directive_groups = true # IDE0055
# IDE0073
file_header_template = Copyright (c) Glory Ltd. All rights reserved.\nConfidential and Proprietary - This software contains confidential and proprietary information\nowned by Glory Ltd. No part of this document may be used, photocopied, reproduced, transmitted,\nor translated to another language without the prior written consent of Glory Ltd.

# this. and Me. preferences
dotnet_style_qualification_for_field = false:suggestion # IDE0003, IDE0009
dotnet_style_qualification_for_property = false:suggestion # IDE0003, IDE0009
dotnet_style_qualification_for_method = false:suggestion # IDE0003, IDE0009
dotnet_style_qualification_for_event = false:suggestion # IDE0003, IDE0009

# Language keywords vs BCL types preferences
dotnet_style_predefined_type_for_locals_parameters_members = true:error # IDE0049
dotnet_style_predefined_type_for_member_access = false:error # IDE0049

# Parentheses preferences
dotnet_style_parentheses_in_arithmetic_binary_operators = always_for_clarity:warning # IDE0047, IDE0048
dotnet_style_parentheses_in_relational_binary_operators = always_for_clarity:warning # IDE0047, IDE0048
dotnet_style_parentheses_in_other_binary_operators = always_for_clarity:warning # IDE0047, IDE0048
dotnet_style_parentheses_in_other_operators = always_for_clarity:warning # IDE0047, IDE0048

# Modifier preferences
dotnet_style_require_accessibility_modifiers = for_non_interface_members:error # IDE0040

# Expression-level preferences
dotnet_style_coalesce_expression = true:warning # IDE0030, IDE0270
dotnet_style_collection_initializer = true:error # IDE0028
dotnet_style_explicit_tuple_names = true:error # IDE0033
dotnet_style_namespace_match_folder = true:error # IDE0130
dotnet_style_null_propagation = true:warning # IDE0031
dotnet_style_object_initializer = true:error # IDE0017
dotnet_style_operator_placement_when_wrapping = beginning_of_line
dotnet_style_prefer_auto_properties = true:error # IDE0032
dotnet_style_prefer_collection_expression = when_types_loosely_match:error # IDE0028, IDE00300, IDE00301, IDE00302, IDE00303, IDE00304, IDE00305
dotnet_style_prefer_compound_assignment = true:warning # IDE0054, IDE0074
dotnet_style_prefer_conditional_expression_over_assignment = true:suggestion # IDE0045
dotnet_style_prefer_conditional_expression_over_return = true:suggestion # IDE0046
dotnet_style_prefer_foreach_explicit_cast_in_source = when_strongly_typed:suggestion # IDE0220
dotnet_style_prefer_inferred_anonymous_type_member_names = true:warning # IDE0037
dotnet_style_prefer_inferred_tuple_names = true:warning # IDE0037
dotnet_style_prefer_is_null_check_over_reference_equality_method = true:error # IDE0041
dotnet_style_prefer_simplified_boolean_expressions = true:error # IDE0075
dotnet_style_prefer_simplified_interpolation = true:error # IDE0071

# Field preferences
dotnet_style_readonly_field = true:warning # IDE0044

# Parameter preferences
dotnet_code_quality_unused_parameters = all:warning # IDE0060

# Suppression preferences
dotnet_remove_unnecessary_suppression_exclusions = none:error # IDE0079

# New line preferences
dotnet_style_allow_multiple_blank_lines_experimental = false:warning
dotnet_style_allow_statement_immediately_after_block_experimental = false:warning

