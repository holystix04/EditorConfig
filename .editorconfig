root = true

[*]
indent_style = space
charset = utf-8
insert_final_newline = true
indent_size = 4
tab_width = 4
trim_trailing_whitespace = true

#=====================================================================================================================================
# YAML/JSON Files
#=====================================================================================================================================
[*.{yaml,yml,json}]
indent_size = 2
tab_width = 2

#=====================================================================================================================================
# XML Files
#=====================================================================================================================================
[*.{xml,slnx,csproj,vbproj,fsproj,dcproj,vcxproj,vcxproj.filters,proj,projitems,shproj,props,targets,ruleset,config,nuspec,resx,vsixmanifest,vsct}]
indent_size = 2
tab_width = 2

#######################
# Resharper rules
#######################
# Formatting Style
resharper_xml_indent_style = space
resharper_xml_indent_size = 2
resharper_xml_tab_width = 2
resharper_xml_alignment_tab_fill_style = use_spaces
resharper_xml_allow_far_alignment = true
resharper_xml_max_line_length = 80
resharper_xml_wrap_tags_and_pi = true
resharper_xml_spaces_around_eq_in_pi_attribute = false
resharper_xml_space_after_last_pi_attribute = false
resharper_xml_pi_attribute_style = do_not_touch
resharper_xml_pi_attributes_indent = align_by_first_attribute
resharper_xml_blank_line_after_pi = false
resharper_xml_spaces_around_eq_in_attribute = false
resharper_xml_space_after_last_attribute = false
resharper_xml_space_before_self_closing = false
resharper_xml_attribute_style = do_not_touch
resharper_xml_attribute_indent = align_by_first_attribute
resharper_xml_keep_user_linebreaks = true
resharper_xml_linebreaks_inside_tags_for_multiline_elements = true
resharper_xml_linebreaks_inside_tags_for_elements_with_child_elements = true
resharper_xml_linebreaks_inside_tags_for_elements_longer_than = 140
resharper_xml_spaces_inside_tags = false
resharper_xml_wrap_text = false
resharper_xml_wrap_around_elements = true
resharper_xml_indent_child_elements = one_indent
resharper_xml_indent_text = one_indent
resharper_xml_max_blank_lines_between_tags = 2
resharper_xml_linebreak_before_multiline_elements = true
resharper_xml_linebreak_before_singleline_elements = false
resharper_xml_insert_final_newline = true

#=====================================================================================================================================
# .NET Files
#=====================================================================================================================================
[*.{cs,csx,vb,vbx,fs}]
max_line_length = 140

